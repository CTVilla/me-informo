---
= partial 'header_information'
.infromation-granted
  .text
    %h3=t('informationgranted.title')
    %p=t('informationgranted.subtitle')
  .container
    .row-column.match-partent
      .column-double
        %input.search{:placeholder => "Buscar", :type => "text", :required => true}
      .column-simple.match-partent
        .dropdown
          %button.dropbtn ¿Dónde?
          .dropdown-content
            %a{:href => "#"} Lugar 1
            %a{:href => "#"} Lugar 2
            %a{:href => "#"} Lugar 3
      .column-simple
        .dropdown
          %button.dropbtn Sector
          .dropdown-content
            %a{:href => "#"} Sector 1
            %a{:href => "#"} Sector 2
            %a{:href => "#"} Sector 3
%hr
.infromation-granted
  .text
    %h3=t('informationgranted.title_section_two')
    %p=t('informationgranted.stadistic_title_html')
