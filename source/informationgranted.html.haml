  .container
    .row-column
      .column_4
        .solid.light-blue-bg.white-text
          2713
          %br
          =t('informationgranted.kpi_section_1')

      .column_4
        .solid.light-blue-text
          4272
          %br
          =t('informationgranted.kpi_section_2')

      .column_4
        .solid.light-blue-text
          2400
          %br
          =t('informationgranted.kpi_section_3')

      .column_4
        .solid.light-blue-text
          4300
          %br
          =t('informationgranted.kpi_section_4')

%hr
